<Window x:Class="podcastination_importer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:podcastination_importer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="320">
    <Grid>
        <Label Content="PodcastKey" 
               HorizontalAlignment="Left" VerticalAlignment="Top" 
               Height="25" Width="96"
               Margin="10,10,0,0" />

        <Label Content="SeasonKey" 
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,40,0,0" />
        
        <Label Content="Title"
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,70,0,0" />
        
        <Label Content="Subtitle" 
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,100,0,0" />
        
        <Label Content="Date" 
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,130,0,0" />
        
        <Label Content="Author" 
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,160,0,0" />
        
        <Label Content="Description" 
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,190,0,0" />
        
        <Label Content="MP3FileName"
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,220,0,0" />
        
        <Label Content="ImageFileName" 
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,250,0,0" />
        
        <Label Content="YouTubeURL" 
               HorizontalAlignment="Left" VerticalAlignment="Top"
               Height="25" Width="96"
               Margin="10,280,0,0" />
        

        <TextBox x:Name="TB_podcatsKey" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,10,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_seasonKey" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,40,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_title" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,70,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_subtitle" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,100,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_date" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25"  Width="179"
                 Margin="111,130,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_author" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,160,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_description" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,190,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_mp3FileName" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,220,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_imageFileName" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,250,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        
        <TextBox x:Name="TB_youTubeURL" 
                 HorizontalAlignment="Left" VerticalAlignment="Top"
                 Height="25" Width="179"
                 Margin="111,280,0,0" 
                 Text="TextBlock" TextWrapping="Wrap" />
        

        <Button x:Name="Btn_start" 
                Content="Start Process" 
                HorizontalAlignment="Center"  VerticalAlignment="Top"
                Height="32" Width="116"
                Margin="0,330,0,0" />

    </Grid>
</Window>
